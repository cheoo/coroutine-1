{
  "name": "zanphp/coroutine",
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "http://packagist.qima-inc.com"
    }
  },
  "require": {
    "php": ">=5.6",
    "zanphp/timer": "dev-master",
    "zanphp/exception": "dev-master",
    "zanphp/testing":"dev-master"
  },
  "autoload": {
    "psr-4": {
      "ZanPHP\\Coroutine\\": "src"
    },
    "files": [
      "src/Command/Task.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ZanPHP\\Coroutine\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "minimum-stability": "dev"
}